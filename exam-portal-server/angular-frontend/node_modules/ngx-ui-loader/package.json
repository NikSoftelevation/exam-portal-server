{
  "name": "ngx-ui-loader",
  "description": "An all-in-one and fully customizable loader/spinner for Angular applications. It supports foreground, background spinner/loader, indicative progress bar and multiple loaders.",
  "version": "13.0.0",
  "schematics": "./schematics/collection.json",
  "ng-save": {
    "save": "dependencies"
  },
  "ng-update": {
    "migrations": "./schematics/migration-collection.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/t-ho/ngx-ui-loader.git"
  },
  "author": "t-ho <t.ho@tdev.app>",
  "homepage": "https://tdev.app/ngx-ui-loader/demo",
  "license": "MIT",
  "private": false,
  "peerDependencies": {
    "@angular/common": ">=13.0.0",
    "@angular/core": ">=13.0.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "keywords": [
    "ngx-ui-loader",
    "ngx-loader",
    "ngx-loading",
    "ngx-multiple-loader",
    "ngx-multiloader",
    "loader",
    "loading",
    "spinner",
    "ngx-spinner",
    "progressbar",
    "progress bar",
    "loader AoT compatible",
    "Angular loader",
    "Angular loading",
    "Angular multiple loaders",
    "Angular spinner",
    "Angular progress bar"
  ],
  "module": "fesm2015/ngx-ui-loader.mjs",
  "es2020": "fesm2020/ngx-ui-loader.mjs",
  "esm2020": "esm2020/ngx-ui-loader.mjs",
  "fesm2020": "fesm2020/ngx-ui-loader.mjs",
  "fesm2015": "fesm2015/ngx-ui-loader.mjs",
  "typings": "ngx-ui-loader.d.ts",
  "exports": {
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./ngx-ui-loader.d.ts",
      "esm2020": "./esm2020/ngx-ui-loader.mjs",
      "es2020": "./fesm2020/ngx-ui-loader.mjs",
      "es2015": "./fesm2015/ngx-ui-loader.mjs",
      "node": "./fesm2015/ngx-ui-loader.mjs",
      "default": "./fesm2020/ngx-ui-loader.mjs"
    }
  },
  "sideEffects": false
}